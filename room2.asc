// room script file

function room_LeaveLeft()
{
  cDet.ChangeRoom(1, 622, 400, eDirectionDown);
}

function room_AfterFadeIn()
{
  if (progress == 0)
  {
    dOpener.Start();
    progress = progress + 1;
  }
  else if (progress == 1)
  {
    dSamplesInc.Start();
  }
  else if (progress == 2)
  {
    dSamplesComp.Start();
    progress = progress + 1;
  }
}
