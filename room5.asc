// room script file

function room_LeaveBottom()
{
  cDet.ChangeRoom(1, 153, 347, eDirectionDown);
}

function oSafe_AnyClick(Object *theObject, CursorMode mode)
{
  foundSafe = true;
  if (paperCode == false)
  {
    cDet.Say("I don't know the code");
  }
  else
  {
    if (fishermanProgress == 2)
    {
      Display("You open the safe and grab the tape before closing it");
      cDet.AddInventory(iTape);
      tapesCollected = true;
      cDet.Say("I should put this into my computer back at the department");
    }
    else
    {
      cDet.Say("I need to ask before I open it");
    }
  }
}